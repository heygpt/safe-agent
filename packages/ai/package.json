    "axios": "^1.7.9",
    "dotenv": "^16.4.7",
    "dotenv-cli": "^8.0.0",
    "express": "^4.21.2",
    "long": "^5.2.4",
    "openai": "^4.77.0",
    "protobufjs": "^7.4.0",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@types/google-protobuf": "^3.15.12",
